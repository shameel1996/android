<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"


        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            android:orientation="horizontal">

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Property ID" />


            </LinearLayout>

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView
                    android:id="@+id/pid"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="03105" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            android:orientation="horizontal">

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="#F1EDED"
                android:orientation="horizontal">

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Property Status" />


            </LinearLayout>

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="#F1EDED"

                android:orientation="horizontal">

                <TextView
                    android:id="@+id/status"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="For Sell" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            android:orientation="horizontal">

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Price" />


            </LinearLayout>

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView
                    android:id="@+id/price"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Price On Call" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            android:orientation="horizontal">

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="#F1EDED"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Min Size" />


            </LinearLayout>

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="#F1EDED"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/size"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            android:orientation="horizontal">

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Max Size" />


            </LinearLayout>

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView
                    android:id="@+id/maxSize"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            android:orientation="horizontal">

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Property Type" />


            </LinearLayout>

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView
                    android:id="@+id/propertyType"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Shop/ Showroom" />


            </LinearLayout>


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            android:orientation="horizontal">

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="#F1EDED"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Rooms" />


            </LinearLayout>

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="#F1EDED"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/rooms"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="4" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            android:orientation="horizontal">

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Bedrooms" />


            </LinearLayout>

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView
                    android:id="@+id/bedrooms"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="2" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            android:orientation="horizontal">

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="#F1EDED"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Bathrooms" />


            </LinearLayout>

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="#F1EDED"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/bathrooms"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="2" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            android:orientation="horizontal">

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Garages" />


            </LinearLayout>

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView
                    android:id="@+id/garages"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="1" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            android:orientation="horizontal">

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="#F1EDED"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Area Type" />


            </LinearLayout>

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="#F1EDED"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/areatype"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Kanal" />


            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"

            android:orientation="horizontal">

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    android:text="Min Price" />


            </LinearLayout>

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView
                    android:id="@+id/minPrice"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="#F1EDED"
            android:orientation="horizontal">

            <LinearLayout


                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_weight="1"

                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp"
                    android:text="Max Price" />


            </LinearLayout>

            <LinearLayout

                android:layout_width="0dp"

                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="#F1EDED"

                android:orientation="horizontal">

                <TextView
                    android:id="@+id/maxPrice"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="4dp" />


            </LinearLayout>


        </LinearLayout>

    </LinearLayout>

</RelativeLayout>